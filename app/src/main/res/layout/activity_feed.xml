<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FeedActivity">


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/main_nav"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/nav_items" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="376dp"
        android:layout_height="245dp"
        android:layout_marginTop="176dp"
        android:background="#005E4C4C"
        android:outlineProvider="bounds"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/View_Feed_Name"
            android:layout_width="200dp"
            android:layout_height="70dp"
            android:outlineProvider="bounds"
            android:singleLine="false"
            android:text="Event Name"
            android:textSize="24sp" />

        <TextView
            android:id="@+id/View_Feed_Date"
            android:layout_width="155dp"
            android:layout_height="60dp"
            android:layout_marginTop="70dp"
            android:outlineProvider="bounds"
            android:text="Date"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/View_Feed_Time"
            android:layout_width="155dp"
            android:layout_height="60dp"
            android:layout_marginStart="155dp"
            android:layout_marginLeft="155dp"
            android:layout_marginTop="70dp"
            android:outlineProvider="bounds"
            android:text="Time"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/View_Feed_Location"
            android:layout_width="155dp"
            android:layout_height="60dp"
            android:layout_marginTop="130dp"
            android:outlineProvider="bounds"
            android:text="Location"
            android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/View_Feed_Att"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_marginStart="270dp"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="150dp"
            android:outlineProvider="bounds"
            android:text="#ATT"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/View_Feed_Tag1"
            android:layout_width="90dp"
            android:layout_height="27dp"
            android:layout_marginTop="210dp"
            android:outlineProvider="bounds"
            android:text="Tag1"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/View_Feed_Tag2"
            android:layout_width="90dp"
            android:layout_height="27dp"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="210dp"
            android:outlineProvider="bounds"
            android:text="Tag2" />

        <TextView
            android:id="@+id/View_Feed_Tag3"
            android:layout_width="90dp"
            android:layout_height="27dp"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="210dp"
            android:outlineProvider="bounds"
            android:text="Tag3" />

        <Button
            android:id="@+id/Favourite "
            android:layout_width="72dp"
            android:layout_height="30dp"
            android:layout_marginLeft="290dp"
            android:text="Favorite"
            android:textSize="8sp" />

    </FrameLayout>

    <TextView
        android:id="@+id/View_Feed_Home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Welcome To JustFriends"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="69dp" />

</androidx.constraintlayout.widget.ConstraintLayout>