<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventDetailsActivity">

    <TextView
        android:id="@+id/textViewEDEventName"
        android:layout_width="182dp"
        android:layout_height="34dp"
        android:text="Event Name"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/textViewEDDetailsBox"
        android:layout_width="353dp"
        android:layout_height="125dp"
        android:text="Details Box Blah Blah"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="173dp" />

    <TextView
        android:id="@+id/textViewEDLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="96dp" />

    <TextView
        android:id="@+id/textViewEDTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="96dp" />

    <TextView
        android:id="@+id/textViewEDDetails"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Details"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="146dp" />

    <TextView
        android:id="@+id/textViewEDDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="96dp" />

    <CheckBox
        android:id="@+id/checkBoxEDFavorite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Favorite"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textViewEDNumGoing"
        android:layout_width="116dp"
        android:layout_height="20dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="255dp"
        android:text="Attendees"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewEDDetailsBox" />

    <TextView
        android:id="@+id/textViewEDInterests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="26dp"
        android:text="Tags"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewEDNumGoing" />

    <TextView
        android:id="@+id/textViewEDInterest1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Interest 1"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="371dp" />

    <TextView
        android:id="@+id/textViewEDInterest2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Interest 2"
        tools:layout_editor_absoluteX="220dp"
        tools:layout_editor_absoluteY="371dp" />

    <TextView
        android:id="@+id/textViewEDInterest3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Interest 3"
        tools:layout_editor_absoluteX="309dp"
        tools:layout_editor_absoluteY="371dp" />

    <Switch
        android:id="@+id/switchGoing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Going"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="438dp" />

</androidx.constraintlayout.widget.ConstraintLayout>