<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#77F4F7D5"
    tools:context=".ProfileInfoActivity">

    <ImageView
        android:id="@+id/imageViewPIProfilePic"
        android:layout_width="108dp"
        android:layout_height="102dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars[5]" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/main_nav"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:state_checked="true"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/nav_items" />

    <TextView
        android:id="@+id/textViewInterestsPI"
        android:layout_width="103dp"
        android:layout_height="33dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Interests"
        android:textColor="#050000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewEducationPI" />

    <TextView
        android:id="@+id/textViewEducationPIOutput"
        android:layout_width="218dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/rubik_mono_one"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewOccupationPIOutput" />

    <TextView
        android:id="@+id/textViewAgePIOutput"
        android:layout_width="218dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="31dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/rubik_mono_one"
        android:hint="MM/DD/YYYY"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewGenderPIOutput" />

    <TextView
        android:id="@+id/textViewEducationPI"
        android:layout_width="103dp"
        android:layout_height="33dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Education"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewOccupationPI" />

    <TextView
        android:id="@+id/textViewDOBPI"
        android:layout_width="103dp"
        android:layout_height="33dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Date of Birth"
        android:textColor="#020000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewGenderPI" />

    <TextView
        android:id="@+id/textViewOccupationPI"
        android:layout_width="103dp"
        android:layout_height="33dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Occupation"
        android:textColor="#0C0000"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewOccupationPIOutput"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewHometownPI" />

    <TextView
        android:id="@+id/textViewOccupationPIOutput"
        android:layout_width="218dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="31dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/rubik_mono_one"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewHometownPIOutput" />

    <Button
        android:id="@+id/buttonEventsPI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="56dp"
        android:background="#4C8569"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Events"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewNamePIOutput" />

    <Button
        android:id="@+id/buttonInfoPI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="17dp"
        android:background="#13462D"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Info"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/buttonEventsPI"
        app:layout_constraintTop_toBottomOf="@+id/textViewNamePIOutput" />

    <TextView
        android:id="@+id/textViewNamePIOutput"
        android:layout_width="172dp"
        android:layout_height="35dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="16dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/rubik_mono_one"
        android:hint="Name"
        android:inputType="textPersonName"
        android:textColor="#070000"
        app:layout_constraintStart_toEndOf="@+id/imageViewPIProfilePic"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonEditProfilePI"
        android:layout_width="66dp"
        android:layout_height="37dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="32dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/rubik_mono_one"
        android:text="edit"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonSignoutPI"
        android:layout_width="116dp"
        android:layout_height="40dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="50dp"
        android:background="#020000"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Sign out"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewInterest1Output" />

    <TextView
        android:id="@+id/textViewGenderPIOutput"
        android:layout_width="218dp"
        android:layout_height="33dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="32dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/rubik_mono_one"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonInfoPI" />

    <TextView
        android:id="@+id/textViewGenderPI"
        android:layout_width="103dp"
        android:layout_height="33dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="25dp"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Gender"
        android:textAlignment="viewStart"
        android:textColor="#050000"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/textViewGenderPIOutput"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonInfoPI" />

    <TextView
        android:id="@+id/textViewInterest1Output"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/rubik_mono_one"
        app:layout_constraintStart_toEndOf="@+id/textViewInterestsPI"
        app:layout_constraintTop_toBottomOf="@+id/textViewEducationPIOutput" />

    <TextView
        android:id="@+id/textViewInterest2Output"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="32dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/rubik_mono_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewEducationPIOutput" />

    <TextView
        android:id="@+id/textViewHometownPI"
        android:layout_width="103dp"
        android:layout_height="33dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/rubik_mono_one"
        android:text="Hometown"
        android:textColor="#070000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewDOBPI" />

    <TextView
        android:id="@+id/textViewHometownPIOutput"
        android:layout_width="218dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="31dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/rubik_mono_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAgePIOutput" />

</androidx.constraintlayout.widget.ConstraintLayout>