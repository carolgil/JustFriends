<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.justfriends">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RecyclerViewGoing"></activity>
        <activity android:name=".FeedActivity" />
        <activity android:name=".EventDetailsActivity" />
        <activity android:name=".CreateEventActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".CreateProfileActivity" />
        <activity android:name=".ProfileFavoritesActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".ProfileEventsActivity" />
        <activity android:name=".ProfileInfoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>